    <%-style %>
</head>
<body>
    
    <%- include('_header'); %>
    <%- script %>

    <script>
          <% if(flash.success && flash.success.length >0) { %>
       
            new Noty({
                theme:'relax',
                text:"<%= flash.success %>",
                type: 'success',
                //  layout : 'topright',
                 timeout : 1500
            }).show();

   <% } %>

   <% if(flash.error && flash.error.length >0) { %>
       
       new Noty({
           theme:'relax',
           text:"<%= flash.error %>",
           type: 'error',
        //  layout : 'topright',
           timeout : 1500
       }).show();

<% } %>


    </script>

    <main id="layout-main">
        <%- body %>
    </main>
        
    

    <%- include('_footer'); %>
   

</body>
